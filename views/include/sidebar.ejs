  <script type="text/javascript">
    function logout() {
        $.ajax({
            url: '/login/logout',
            method: 'GET',
            cache: false,
            beforeSend: function (xhr) {
                xhr.withCredentials = true;
            },
            success: function (data, textStatus, xhr) {
                alert('로그아웃 되었습니다.');
                window.location.href="/login";
            },
            error: function (data, textStatus, xhr) {
                alert(data);
            }
        });
    }
  </script>
  
